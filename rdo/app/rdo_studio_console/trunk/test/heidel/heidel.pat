



$Pattern Образец_генерация_бревен  : rule   trace
$Relevant_resources
  система       : Распилочная_машина_1   Keep
  транспортер   : Транспортер            Keep
  новое_бревно  : Бревно                 Create
$Body
  система
    Choice NoCheck first
    Convert_rule
      счетчик_бревен   = система.счетчик_бревен + 1;
  транспортер
    Choice from транспортер.счетчик > 0
    first
    Convert_rule
      текущее_кол_бревен = транспортер.текущее_кол_бревен + 1;
      счетчик          = транспортер.счетчик - 1;
      ген_длина        = Длина_бревна(транспортер.код);
      ген_диаметр      = Диаметр_бревна(транспортер.код);
  новое_бревно
    Convert_rule   trace
      номер            = система.счетчик_бревен;
      длина            = транспортер.ген_длина;
      диаметр_н        = транспортер.ген_диаметр;
      диаметр_к        = транспортер.ген_диаметр - (транспортер.ген_длина * 10) / 100;
      объем_кубический_м  = Объем_бревна(транспортер.ген_длина, транспортер.ген_диаметр,
                                транспортер.ген_диаметр - (транспортер.ген_длина * 10) / 100);
      нач_длина        = транспортер.ген_длина;
      нач_объем_кубический_м   = Объем_бревна(транспортер.ген_длина, транспортер.ген_диаметр,
                                     транспортер.ген_диаметр - (транспортер.ген_длина * 10) / 100);
      вид_древесины    = транспортер.получить_вид_древесины;
      качество         = транспортер.получить_качество;
      статус           = ничего;
      код_места        = транспортер.код;
      счетчик_оборотов = 0;
      позиция          = 0;
      использование_длины  = 0.0;
      использование_куб_м  = 0.0;
$End

$Pattern Образец_прибытие_заказа  : rule   trace
$Relevant_resources
  некий_заказ      : Позиция_заказа        Keep
  заказ_для_статис : Заказ_для_статистики  Create
$Body
  некий_заказ
    Choice from некий_заказ.статус == обработано
    first
    Convert_rule
      номер_заказа     NoChange;
      номер_позиции    NoChange;
      имя_клиента      = Имя_клиента;
      количество       = Количество_заказов(10, 70);
      дата_обслуживания  = Time_now + Дата_выполнения_заказа (1, 5) * 8.0;
      время_прибытия   = Time_now;
      типоразмер       = Типоразмер_пиломатериала; 
      длина            = Длина_пиломатериала(12, 24) * 25;
      ширина           = Размеры_пиломатериала(_ширина, некий_заказ.типоразмер);
      высота           = Размеры_пиломатериала(_высота, некий_заказ.типоразмер);
      объем            = (некий_заказ.длина / 100.0) * (некий_заказ.ширина / 1000.0) *
                           (некий_заказ.высота / 1000.0) * некий_заказ.количество;
      вид_древесины    = Вид_древесины_заказа;
      класс            = Класс_заказа;
      качество         = Качество_заказа;
      приоритет        = Приоритет_заказа(1, 10);
      кол_готовых      = 0;
      кол_пилоблоков   = 0;
      склад            = проходит;
      статус           = в_обработке;
  заказ_для_статис
    Convert_rule   trace
      номер_заказа     = некий_заказ.номер_заказа;
      номер_позиции    = некий_заказ.номер_позиции;
      дата_обслуживания  = некий_заказ.дата_обслуживания;
      время_прибытия   = Time_now;
      количество       = некий_заказ.количество;
      кол_готовых      = 0;
      кол_пилоблоков   = 0;
      треб_диам_1      = Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс, 1) +
                              некий_заказ.длина / 10.0;
      треб_диам_2      = Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс, 2) +
                              некий_заказ.длина / 10.0;
      треб_диам_3      = Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс, 3) +
                              некий_заказ.длина / 10.0;
      треб_диам_4      = Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс, 4) +
                              некий_заказ.длина / 10.0;
      треб_диам_6      = Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс, 6) +
                              некий_заказ.длина / 10.0;
      треб_диам_8      = Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс, 8) +
                              некий_заказ.длина / 10.0;
      треб_диам_9      = Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс, 9) +
                              некий_заказ.длина / 10.0;
$End

$Pattern Образец_выбор_склада  : rule   trace
$Relevant_resources
  некий_заказ      : Позиция_заказа    Keep
  некоторый_склад  : Склад             Keep
$Body
  некий_заказ
    Choice from некий_заказ.статус == в_обработке and
                некий_заказ.склад == проходит
    first
    Convert_rule
      склад            = некоторый_склад.код;
  некоторый_склад
    Choice from некоторый_склад.код <> Ск_29 and некоторый_склад.код <> Ск_30 and
                некоторый_склад.номер_заказа == 0 and
                некоторый_склад.длина * 100 >= некий_заказ.длина
    first
    Convert_rule
      номер_заказа     = некий_заказ.номер_заказа;
      номер_позиции    = некий_заказ.номер_позиции;
$End

$Pattern  Образец_Бревно_переходит_Т2_К2  : operation  trace
$Relevant_resources
  транспортер       : Транспортер_2    Keep      Keep
  некоторое_бревно  : Бревно           Keep      Keep
  конвейер          : Конвейер_2       Keep      Keep
  показать_бревно   : Бревно_показать  Keep      Keep
$Time = Время_передачи_бревна
$Body
  транспортер
    Choice from транспортер.текущее_кол_бревен > 0 and
                транспортер.статус == остановлен
    first
    Convert_begin
      статус           = вперед;
    Convert_end
      текущее_кол_бревен  = транспортер.текущее_кол_бревен - 1;
      статус           = остановлен;
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Тр_2
    first
    Convert_begin
      код_места        = проходит;
    Convert_end
      код_места        = Кн_2;
      счетчик_оборотов = конвейер.счетчик_оборотов;
      позиция          = конвейер.позиция + 900;
  конвейер
    Choice from конвейер.текущее_кол_бревен == 0 and
                конвейер.движение == нет and
                Конвейер_1.текущее_кол_бревен == 0
    first
    Convert_begin
      текущее_кол_бревен  = конвейер.текущее_кол_бревен + 1;
      статус           = остановлен;
    Convert_end
      шаг              = Конв_2_нормальный_шаг;
      статус           = вперед;
  показать_бревно
    Choice from показать_бревно.номер == 0
    first
    Convert_begin
      номер            = некоторое_бревно.номер;
      длина            = 0;
    Convert_end
      длина            = некоторое_бревно.длина;
      код_места        = некоторое_бревно.код_места;
      счетчик_оборотов = некоторое_бревно.счетчик_оборотов;
      позиция          = некоторое_бревно.позиция;
$End

$Pattern  Образец_Бревно_переходит_Т1_К1 : operation  trace
$Relevant_resources
  транспортер       : Транспортер_1    Keep      Keep
  некоторое_бревно  : Бревно           Keep      Keep
  конвейер          : Конвейер_1       Keep      Keep
  показать_бревно   : Бревно_показать  Keep      Keep
$Time = Время_передачи_бревна
$Body
  транспортер
    Choice from транспортер.текущее_кол_бревен > 0 and
                транспортер.статус == остановлен
    first
    Convert_begin
      статус           = вперед;
    Convert_end
      текущее_кол_бревен  = транспортер.текущее_кол_бревен - 1;
      статус           = остановлен;
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Тр_1
    first
    Convert_begin
      код_места        = проходит;
    Convert_end
      код_места        = Кн_1;
      счетчик_оборотов = конвейер.счетчик_оборотов;
      позиция          = конвейер.позиция + 500;
  конвейер
    Choice from конвейер.текущее_кол_бревен == 0 and
                конвейер.движение == нет and
                Конвейер_2.текущее_кол_бревен == 0
    first
    Convert_begin
      текущее_кол_бревен  = конвейер.текущее_кол_бревен + 1;
      статус           = остановлен;
    Convert_end
      статус           = вперед;
  показать_бревно
    Choice from показать_бревно.номер == 0
    first
    Convert_begin
      номер            = некоторое_бревно.номер;
      длина            = 0;
    Convert_end
      длина            = некоторое_бревно.длина;
      код_места        = некоторое_бревно.код_места;
      счетчик_оборотов = некоторое_бревно.счетчик_оборотов;
      позиция          = некоторое_бревно.позиция;
$End

$Pattern  Образец_Бревно_переходит_К1_К2  : operation  trace
$Relevant_resources
  некоторое_бревно      : Бревно            Keep      Keep
  конвейер_откуда       : Конвейер_1        Keep      Keep
  конвейер_куда         : Конвейер_2        NoChange  Keep
  показать_бревно       : Бревно_показать   NoChange  Keep
$Time = Время_передачи_бревна
$Body
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_1 and
                Конвейер_1.позиция - некоторое_бревно.позиция -
                (Конвейер_1.счетчик_оборотов - некоторое_бревно.счетчик_оборотов - 1) *
                Конвейер_1.длина <= 0
    first
    Convert_begin
      код_места        = проходит;
    Convert_end
      код_места        = Кн_2;
      счетчик_оборотов = конвейер_куда.счетчик_оборотов;
      позиция          = конвейер_куда.позиция;
  конвейер_откуда
    Choice from конвейер_откуда.текущее_кол_бревен > 0 and
                конвейер_откуда.движение == нет
    first
    Convert_begin
      статус           = остановлен;
    Convert_end
      текущее_кол_бревен  = конвейер_откуда.текущее_кол_бревен - 1;
      статус           = вперед;
  конвейер_куда
    Choice from конвейер_куда.текущее_кол_бревен == 0
    first
    Convert_end
      текущее_кол_бревен  = конвейер_куда.текущее_кол_бревен + 1;
  показать_бревно
    Choice from показать_бревно.номер == некоторое_бревно.номер
    first
    Convert_end
      код_места        = некоторое_бревно.код_места;
      счетчик_оборотов = некоторое_бревно.счетчик_оборотов;
      позиция          = некоторое_бревно.позиция;
$End

$Pattern Образец_начало_измерения  : rule   trace
$Relevant_resources
  позиция_измерения : Позиция_измерения_1   Keep
  некоторое_бревно  : Бревно                Keep
$Body
  позиция_измерения
    Choice from позиция_измерения.состояние == свободен
    first
    Convert_rule
      состояние        = занят;
      длина            = 0;
      диаметр_н        = некоторое_бревно.диаметр_н;
      диаметр_к        = некоторое_бревно.диаметр_н;
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_2 and
                некоторое_бревно.статус == ничего and
                Конвейер_2.позиция - некоторое_бревно.позиция -
                (Конвейер_2.счетчик_оборотов - некоторое_бревно.счетчик_оборотов - 1) *
                Конвейер_2.длина <= ПИ_координата
    first
    Convert_rule
      статус           = измерение;
$End

$Pattern Образец_конец_измерения  : rule   trace
$Relevant_resources
  позиция_измерения   : Позиция_измерения_1    Keep
  некоторое_бревно    : Бревно                 Keep
  распилочная_машина  : Распилочная_машина_1   Keep
  конвейер            : Конвейер_2             Keep
$Body
  позиция_измерения
    Choice from позиция_измерения.состояние == занят
    first
    Convert_rule
      состояние        = свободен;
      длина            = некоторое_бревно.длина;
      диаметр_н        = некоторое_бревно.диаметр_н;
      диаметр_к        = некоторое_бревно.диаметр_к;
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_2 and
                некоторое_бревно.статус == измерение and
                Конвейер_2.позиция - некоторое_бревно.позиция -
                (Конвейер_2.счетчик_оборотов - некоторое_бревно.счетчик_оборотов - 1) *
                Конвейер_2.длина + некоторое_бревно.длина <= ПИ_координата
    first
    Convert_rule
      статус           = раскрой;
  распилочная_машина
    Choice NoCheck first
    Convert_rule
      длина            = некоторое_бревно.длина;
      диаметр_н        = некоторое_бревно.диаметр_н;
      диаметр_к        = некоторое_бревно.диаметр_к;
      объем_кубический_м  = некоторое_бревно.объем_кубический_м;
      длина_брутто     = некоторое_бревно.длина;
      длина_нетто      = 0;
      объем_кубический_м_брутто     = некоторое_бревно.объем_кубический_м;
      объем_кубический_м_нетто      = 0.0;
      длина_для_отпила      = некоторое_бревно.длина - Размер_отпил_торца;
      диаметр_н_для_отпила  = некоторое_бревно.диаметр_н;
  конвейер
    Choice NoCheck first
    Convert_rule
      шаг              = Конв_2_медленный_шаг;
$End

$Pattern Образец_раскрой  : rule   trace
$Relevant_resources
  некоторое_бревно    : Бревно                NoChange
  результат           : Результат_раскроя     Keep
  некий_заказ         : Позиция_заказа        Keep
  заказ_для_статис    : Заказ_для_статистики  Keep
  конвейер            : Конвейер_2            NoChange
  распилочная_машина  : Распилочная_машина_1  Keep
$Body
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_2 and
                некоторое_бревно.статус == раскрой
    first
  результат
    Choice from результат.номер == распилочная_машина.кол_пилоблоков + 1
    first
    Convert_rule
      номер_заказа     = некий_заказ.номер_заказа;
      номер_позиции    = некий_заказ.номер_позиции;
      имя_клиента      = некий_заказ.имя_клиента;
      вид_древесины    = некоторое_бревно.вид_древесины;
      качество         = некий_заказ.качество;
      класс            = некий_заказ.класс;
      длина            = некий_заказ.длина;
      ширина           = некий_заказ.ширина;
      высота           = некий_заказ.высота;
      кол_пилоблоков   = IMin(Получить_кол_брусьев(некий_заказ.типоразмер, некий_заказ.класс,
                             распилочная_машина.диаметр_н_для_отпила - некий_заказ.длина / 10.0),
                             некий_заказ.количество - некий_заказ.кол_готовых);
      тек_диаметр       = распилочная_машина.диаметр_н_для_отпила;
      треб_диам         = Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс,
                             Получить_кол_брусьев(некий_заказ.типоразмер, некий_заказ.класс,
                               распилочная_машина.диаметр_н_для_отпила - некий_заказ.длина / 10.0)) +
                             (некий_заказ.длина * 10) / 100;
      объем_кубический_м  = Объем_бревна(некий_заказ.длина, распилочная_машина.диаметр_н_для_отпила,
                             распилочная_машина.диаметр_н_для_отпила - (некий_заказ.длина * 10) / 100);
      использование       = результат.кол_пилоблоков * (некий_заказ.длина / 100.0 *
                              некий_заказ.ширина / 1000000.0 * некий_заказ.высота) /
                              результат.объем_кубический_м;
      код_склада       = некий_заказ.склад;
      termin           = некий_заказ.дата_обслуживания;
      soll             = некий_заказ.количество;
      lst              = некий_заказ.кол_готовых + результат.кол_пилоблоков;
      накопл_длина     = распилочная_машина.длина_нетто + некий_заказ.длина + Размер_отпил_торца;
  некий_заказ
    Choice from некий_заказ.статус == в_обработке and
                некий_заказ.склад <> проходит and
                некий_заказ.кол_готовых < некий_заказ.количество and
                некий_заказ.длина <= распилочная_машина.длина_для_отпила and
                Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс, 1) +
                  некий_заказ.длина / 10.0 <= распилочная_машина.диаметр_н_для_отпила
    with_max (некий_заказ.длина / 100.0 * некий_заказ.ширина /
             1000000.0 * некий_заказ.высота *
             IMin(Получить_кол_брусьев(некий_заказ.типоразмер, некий_заказ.класс,
               распилочная_машина.диаметр_н_для_отпила - некий_заказ.длина / 10.0),
               некий_заказ.количество - некий_заказ.кол_готовых)) /
             Объем_бревна(некий_заказ.длина, распилочная_машина.диаметр_н_для_отпила,
               распилочная_машина.диаметр_н_для_отпила - (некий_заказ.длина * 10) / 100)

/*    with_min распилочная_машина.диаметр_н_для_отпила - (некий_заказ.длина * 10) / 100 -
             Требуемый_диаметр(некий_заказ.типоразмер, некий_заказ.класс,
               IMin(Получить_кол_брусьев(некий_заказ.типоразмер, некий_заказ.класс,
               распилочная_машина.диаметр_н_для_отпила - некий_заказ.длина / 10.0),
               некий_заказ.количество - некий_заказ.кол_готовых))*/

/*    with_max Случайно(0.0, 1.0)*/

    Convert_rule
      кол_готовых      = некий_заказ.кол_готовых + результат.кол_пилоблоков;
      кол_пилоблоков   = некий_заказ.кол_пилоблоков + 1;
  заказ_для_статис
    Choice from заказ_для_статис.номер_заказа == некий_заказ.номер_заказа and
                заказ_для_статис.номер_позиции == некий_заказ.номер_позиции
    first
    Convert_rule
      кол_готовых      = некий_заказ.кол_готовых;
      кол_пилоблоков   = некий_заказ.кол_пилоблоков;
  конвейер
    Choice from конвейер.текущее_кол_бревен > 0 and
                конвейер.движение == нет
    first
  распилочная_машина
    Choice from распилочная_машина.состояние == свободен
    first
    Convert_rule
      кол_пилоблоков   = распилочная_машина.кол_пилоблоков + 1;
      длина_нетто      = распилочная_машина.длина_нетто + некий_заказ.длина;
      объем_кубический_м_нетто  = распилочная_машина.объем_кубический_м_нетто +
                                    Объем_бревна(некий_заказ.длина, распилочная_машина.диаметр_н_для_отпила,
                                    распилочная_машина.диаметр_н_для_отпила - (некий_заказ.длина * 10) / 100);
      длина_для_отпила      = распилочная_машина.длина_для_отпила - некий_заказ.длина;
      диаметр_н_для_отпила  = распилочная_машина.диаметр_н_для_отпила - (некий_заказ.длина * 10) / 100;
$End

$Pattern Образец_конец_раскроя  : rule   trace
$Relevant_resources
  некоторое_бревно          : Бревно             Keep
$Body
