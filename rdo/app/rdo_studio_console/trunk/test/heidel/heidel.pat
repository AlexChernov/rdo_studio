$Pattern Образец_переключение_назад  : rule   trace
$Relevant_resources
  некоторое_бревно  : Бревно          NoChange
  конвейер          : Конвейер_2      Keep
$Body
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_2 and
                некоторое_бревно.статус == раскроено and
                Конвейер_2.позиция - некоторое_бревно.позиция -
                (Конвейер_2.счетчик_оборотов - некоторое_бревно.счетчик_оборотов - 1) *
                Конвейер_2.длина < Размер_отпил_торца * -1
    first
  конвейер
    Choice from конвейер.статус == вперед and конвейер.движение == нет
    first
    Convert_rule
      статус           = назад;
$End

$Pattern Образец_переключение_стоп  : rule   trace
$Relevant_resources
  некоторое_бревно  : Бревно          Keep
  конвейер          : Конвейер_2      Keep
$Body
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_2 and
                некоторое_бревно.статус == раскроено and
                Конвейер_2.позиция - некоторое_бревно.позиция -
                (Конвейер_2.счетчик_оборотов - некоторое_бревно.счетчик_оборотов - 1) *
                Конвейер_2.длина == Размер_отпил_торца * -1
    first
    Convert_rule
      статус           = готово_к_отпилке_торца;
  конвейер
    Choice from конвейер.движение == нет
    first
    Convert_rule
      статус           = остановлен;
$End

$Pattern Образец_зачистка_торца  : operation   trace
$Relevant_resources
  распилочная_машина : Распилочная_машина_1     Keep      Keep
  некоторое_бревно   : Бревно                   NoChange  Keep
  показать_бревно    : Бревно_показать          NoChange  Keep
  конвейер           : Конвейер_2               NoChange  Keep
$Time = Время_распиловки
$Body
  распилочная_машина
    Choice from распилочная_машина.состояние == свободен
    first
    Convert_begin
      состояние        = занят;
    Convert_end
      состояние        = свободен;
      длина            = распилочная_машина.длина - Размер_отпил_торца;
      диаметр_н        = распилочная_машина.диаметр_н - Размер_отпил_торца * 10 / 100;
      объем_кубический_м  = Объем_бревна(распилочная_машина.длина,
                              распилочная_машина.диаметр_н, распилочная_машина.диаметр_к);
      тек_номер_пилоблоков  = 1;
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_2 and
                некоторое_бревно.статус == готово_к_отпилке_торца
    first
    Convert_end
      длина            = распилочная_машина.длина;
      диаметр_н        = распилочная_машина.диаметр_н;
      объем_кубический_м  = распилочная_машина.объем_кубический_м;
      статус           = позиционируется;
      счетчик_оборотов = некоторое_бревно.счетчик_оборотов +
                             Новый_счетчик_оборотов_бревно(некоторое_бревно.позиция,
                             Размер_отпил_торца, конвейер.длина);
      позиция          = Новая_позиция_бревно(некоторое_бревно.позиция,
                             Размер_отпил_торца, конвейер.длина);
  показать_бревно
    Choice from показать_бревно.номер == некоторое_бревно.номер
    first
    Convert_end
      длина            = некоторое_бревно.длина;
      счетчик_оборотов = некоторое_бревно.счетчик_оборотов;
      позиция          = некоторое_бревно.позиция;
  конвейер
    Choice from конвейер.текущее_кол_бревен > 0 and
                конвейер.статус == остановлен
    first
    Convert_end
      шаг              = Конв_2_нормальный_шаг;
      статус           = вперед;
$End

$Pattern Образец_переключение_позиционировано  : rule   trace
$Relevant_resources
  некоторое_бревно  : Бревно             Keep
  результат         : Результат_раскроя  NoChange
  конвейер          : Конвейер_2         Keep
$Body
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_2 and
                некоторое_бревно.статус == позиционируется and
                Распилочная_машина_1.тек_номер_пилоблоков <= Распилочная_машина_1.кол_пилоблоков
    first
    Convert_rule
      статус           = позиционировано;
  результат
    Choice from результат.номер == Распилочная_машина_1.тек_номер_пилоблоков and
                Конвейер_2.позиция - некоторое_бревно.позиция -
                (Конвейер_2.счетчик_оборотов - некоторое_бревно.счетчик_оборотов - 1) *
                Конвейер_2.длина <= результат.длина * -1
    first
  конвейер
    Choice from конвейер.движение == нет
    first
    Convert_rule
      статус           = остановлен;
$End

$Pattern Образец_распилка  : operation   trace
$Relevant_resources
  конвейер_2                : Конвейер_2            NoChange  NoChange
  результат                 : Результат_раскроя     NoChange  NoChange
  распилочная_машина        : Распилочная_машина_1  Keep      Keep
  некоторое_бревно          : Бревно                Keep      Keep
  показать_бревно           : Бревно_показать       NoChange  Keep
  некоторый_пилоблок_бревна : Пилоблоки             Create    NoChange
  конвейер_3                : Конвейер_3            NoChange  Keep
  новое_показать_бревно     : Бревно_показать       Keep      Keep
$Time = Время_распиловки
$Body
  конвейер_2
    Choice from конвейер_2.текущее_кол_бревен > 0 and
                конвейер_2.статус == остановлен
    first
  результат
    Choice from результат.номер == распилочная_машина.тек_номер_пилоблоков
    first
  распилочная_машина
    Choice from распилочная_машина.состояние == свободен
    first
    Convert_begin
      состояние        = занят;
      счетчик_бревен   = распилочная_машина.счетчик_бревен + 1;
    Convert_end
      состояние        = свободен;
      длина            = распилочная_машина.длина - результат.длина;
      диаметр_н        = распилочная_машина.диаметр_н - результат.длина * 10 / 100;
      объем_кубический_м  = Объем_бревна(распилочная_машина.длина,
                                распилочная_машина.диаметр_н, распилочная_машина.диаметр_к);
      тек_номер_пилоблоков   = распилочная_машина.тек_номер_пилоблоков + 1;
      обработано_пилоблоков  = распилочная_машина.обработано_пилоблоков + 1;
      общий_объем_кубический_м_выхода  = распилочная_машина.общий_объем_кубический_м_выхода + результат.кол_пилоблоков *
                                           (результат.длина / 100.0 * результат.ширина / 1000000.0 *
                                            результат.высота);
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_2 and
                некоторое_бревно.статус == позиционировано
    first
    Convert_begin
      статус           = распиловка;
    Convert_end
      длина            = распилочная_машина.длина;
      диаметр_н        = распилочная_машина.диаметр_н;
      объем_кубический_м  = распилочная_машина.объем_кубический_м;
      статус           = позиционируется;
      счетчик_оборотов = некоторое_бревно.счетчик_оборотов +
                             Новый_счетчик_оборотов_бревно(некоторое_бревно.позиция,
                             результат.длина, конвейер_2.длина);
      позиция          = Новая_позиция_бревно(некоторое_бревно.позиция,
                             результат.длина, конвейер_2.длина);
  показать_бревно
    Choice from показать_бревно.номер == некоторое_бревно.номер
    first
    Convert_end
      длина            = некоторое_бревно.длина;
      счетчик_оборотов = некоторое_бревно.счетчик_оборотов;
      позиция          = некоторое_бревно.позиция;
  некоторый_пилоблок_бревна
    Convert_begin  trace
      номер            = распилочная_машина.счетчик_бревен;
      длина            = результат.длина;
      диаметр_н        = распилочная_машина.диаметр_н;
      диаметр_к        = распилочная_машина.диаметр_н - результат.длина * 10 / 100;
      объем_кубический_м  = результат.объем_кубический_м;
      вид_древесины    = результат.вид_древесины;
      качество         = результат.качество;
      код_места        = Кн_3;
      позиция          = Конвейер_3.позиция + результат.длина;
      счетчик_оборотов = Конвейер_3.счетчик_оборотов;
      номер_заказа     = результат.номер_заказа;
      номер_позиции    = результат.номер_позиции;
      код_склада       = результат.код_склада;
      кол_пилоблоков   = результат.кол_пилоблоков;
      треб_диам        = результат.треб_диам;
      использование    = результат.использование;
  конвейер_3
    Choice from конвейер_3.текущее_кол_бревен == 0
    first
    Convert_end
      текущее_кол_бревен  = конвейер_3.текущее_кол_бревен + 1;
      статус           = вперед;
  новое_показать_бревно
    Choice from новое_показать_бревно.номер == 0
    first
    Convert_begin
      номер            = распилочная_машина.счетчик_бревен;
      длина            = 0;
    Convert_end
      длина            = некоторый_пилоблок_бревна.длина;
      код_места        = некоторый_пилоблок_бревна.код_места;
      счетчик_оборотов = некоторый_пилоблок_бревна.счетчик_оборотов;
      позиция          = некоторый_пилоблок_бревна.позиция;
$End

$Pattern  Образец_Бревно_переходит_К2_К3  : operation  trace
$Relevant_resources
  некоторое_бревно    : Бревно                Keep      Keep
  конвейер_откуда     : Конвейер_2            Keep      Keep
  конвейер_куда       : Конвейер_3            NoChange  Keep
  показать_бревно     : Бревно_показать       NoChange  Keep
  распилочная_машина  : Распилочная_машина_1  NoChange  Keep
  позиция_измерения   : Позиция_измерения_1   NoChange  Keep
$Time = Время_передачи_бревна
$Body
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Кн_2 and
                некоторое_бревно.статус == позиционируется and
                Распилочная_машина_1.тек_номер_пилоблоков > Распилочная_машина_1.кол_пилоблоков
    first
    Convert_begin
      код_места        = проходит;
    Convert_end
      статус           = готово;
      код_места        = Кн_3;
      счетчик_оборотов = конвейер_куда.счетчик_оборотов;
      позиция          = конвейер_куда.позиция;
      использование_длины  = (некоторое_бревно.нач_длина - некоторое_бревно.длина) * 1.0 / некоторое_бревно.нач_длина;
      использование_куб_м  = (некоторое_бревно.нач_объем_кубический_м - некоторое_бревно.объем_кубический_м) / некоторое_бревно.нач_объем_кубический_м;
  конвейер_откуда
    Choice from конвейер_откуда.текущее_кол_бревен > 0 and
                конвейер_откуда.движение == нет
    first
    Convert_begin
      текущее_кол_бревен  = конвейер_откуда.текущее_кол_бревен - 1;
      статус           = остановлен;
    Convert_end
      статус           = вперед;
  конвейер_куда
    Choice from конвейер_куда.текущее_кол_бревен == 0
    first
    Convert_end
      текущее_кол_бревен  = конвейер_куда.текущее_кол_бревен + 1;
  показать_бревно
    Choice from показать_бревно.номер == некоторое_бревно.номер
    first
    Convert_end
      код_места        = некоторое_бревно.код_места;
      счетчик_оборотов = некоторое_бревно.счетчик_оборотов;
      позиция          = некоторое_бревно.позиция;
  распилочная_машина
    Choice NoCheck first
    Convert_end
      длина            = 0;
      диаметр_н        = 0;
      диаметр_к        = 0;
      кол_пилоблоков   = 0;
      тек_номер_пилоблоков = 0;
      длина_нетто      = 0;
      объем_кубический_м_нетто  = 0.0;
  позиция_измерения
    Choice NoCheck first
    Convert_end
      длина            = 0;
      диаметр_н        = 0;
      диаметр_к        = 0;
$End

$Pattern  Образец_Бревно_переходит_К3_Т3  : operation  trace
$Relevant_resources
  некоторый_пилоблок_бревна : Пилоблоки         Keep      Keep
  конвейер                  : Конвейер_3        Keep      Keep
  транспортер               : Транспортер_3     NoChange  Keep
  конвейер_2                : Конвейер_2        NoChange  Keep
  показать_бревно           : Бревно_показать   NoChange  Keep
$Time = Время_передачи_бревна
$Body
  некоторый_пилоблок_бревна
    Choice from некоторый_пилоблок_бревна.код_места == Кн_3 and
                Конвейер_3.позиция - некоторый_пилоблок_бревна.позиция -
                (Конвейер_3.счетчик_оборотов - некоторый_пилоблок_бревна.счетчик_оборотов - 1) *
                Конвейер_3.длина <= 200
    first
    Convert_begin
      код_места        = проходит;
    Convert_end
      код_места        = Тр_3;
      позиция          = транспортер.позиция;
      счетчик_оборотов = транспортер.счетчик_оборотов;
  конвейер
    Choice from конвейер.текущее_кол_бревен > 0 and
                конвейер.движение == нет
    first
    Convert_begin
      статус           = остановлен;
    Convert_end
      текущее_кол_бревен  = конвейер.текущее_кол_бревен - 1;
      статус           = вперед;
  транспортер
    Choice NoCheck first
    Convert_end
      текущее_кол_бревен  = транспортер.текущее_кол_бревен + 1;
  конвейер_2
    Choice NoCheck first
    Convert_end
      статус           = вперед;
  показать_бревно
    Choice from показать_бревно.номер == некоторый_пилоблок_бревна.номер
    first
    Convert_end
      код_места        = некоторый_пилоблок_бревна.код_места;
      счетчик_оборотов = некоторый_пилоблок_бревна.счетчик_оборотов;
      позиция          = некоторый_пилоблок_бревна.позиция;
$End

$Pattern  Образец_Бревно_переходит_К3_С30  : operation  trace
$Relevant_resources
  некоторое_бревно    : Бревно            Keep      Keep
  конвейер            : Конвейер_3        Keep      Keep
