    first
    Convert_end
      номер            = 0;
      длина            = 0;
$End

$Pattern Образец_уничтожение_остатка_бревна  : rule   trace
$Relevant_resources
  система           : Распилочная_машина_1   Keep
  некоторое_бревно  : Бревно                 Erase
$Body
  система
    Choice NoCheck first
    Convert_rule
      бревна_обработаны      = система.бревна_обработаны + 1;
      общая_длина_бревен     = система.общая_длина_бревен + некоторое_бревно.нач_длина / 100.0;
      общая_длина_остатка    = система.общая_длина_остатка + некоторое_бревно.длина / 100.0;
      общий_объем_кубический_м_бревен = система.общий_объем_кубический_м_бревен + некоторое_бревно.нач_объем_кубический_м;
      общий_объем_кубический_м_остатка   = система.общий_объем_кубический_м_остатка + некоторое_бревно.объем_кубический_м;
  некоторое_бревно
    Choice from некоторое_бревно.код_места == Ск_30 and
                некоторое_бревно.статус == готово
    first
$End

$Pattern  Образец_Бревно_переходит_Т3_К4  : operation  trace
$Relevant_resources
  транспортер               : Транспортер_3     Keep      Keep
  некоторый_пилоблок_бревна : Пилоблоки         Keep      Keep
  конвейер                  : Конвейер_4        NoChange  Keep
  показать_бревно           : Бревно_показать   NoChange  Keep
$Time = Время_передачи_бревна
$Body
  транспортер
    Choice from транспортер.текущее_кол_бревен > 0 and
                транспортер.движение == нет
    first
    Convert_begin
      статус           = остановлен;
    Convert_end
      текущее_кол_бревен  = транспортер.текущее_кол_бревен - 1;
      статус           = вперед;
  некоторый_пилоблок_бревна
    Choice from некоторый_пилоблок_бревна.код_места == Тр_3 and
                Транспортер_3.позиция - некоторый_пилоблок_бревна.позиция -
                (Транспортер_3.счетчик_оборотов - некоторый_пилоблок_бревна.счетчик_оборотов - 1) *
                Транспортер_3.длина <= 0
    first
    Convert_begin
      код_места        = проходит;
    Convert_end
      код_места        = Кн_4;
      позиция          = конвейер.позиция + 200;
      счетчик_оборотов = конвейер.счетчик_оборотов;
  конвейер
    Choice from конвейер.текущее_кол_бревен == 0
    first
    Convert_end
      текущее_кол_бревен  = конвейер.текущее_кол_бревен + 1;
  показать_бревно
    Choice from показать_бревно.номер == некоторый_пилоблок_бревна.номер
    first
    Convert_end
      код_места        = некоторый_пилоблок_бревна.код_места;
      счетчик_оборотов = некоторый_пилоблок_бревна.счетчик_оборотов;
      позиция          = некоторый_пилоблок_бревна.позиция;
$End

