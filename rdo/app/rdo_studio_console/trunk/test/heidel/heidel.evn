$Pattern Образец_Прибытие_машины_1  : event   trace
$Relevant_resources
  транспортер_1 : Транспортер_1  Keep
  
$Body
  транспортер_1
    Convert_event
      счетчик                  = Получить_количество_в_машине_1(15, 45);
      получить_вид_древесины   = Вид_древесины_бревна;
      получить_качество        = Качество_бревна;
		Образец_Прибытие_машины_1.Planning( Time_now + Вход_1(Средний_интервал_1) );
$End

