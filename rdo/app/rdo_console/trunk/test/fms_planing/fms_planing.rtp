$Resource_type Цеха: permanent
$Parameters
	количество_пришедших    : integer = 0
	количество_в_очереди    : integer = 0  
	количество_обработанных : integer = 0
$End 
 
$Resource_type Станки: permanent 
$Parameters
	состояние_станка           : ( Свободен, Занят, Подготовлен, Простой, Установка_базирование ) = Свободен
	тип                        : (нет, ток, фр)
	количество_обработанных    : integer = 0
	//номер_обработанной_детали  : integer = 1  // не обрабатывалось 
	номер_текущей_детали       : integer = 1
	номер_текущей_операции     : integer = 0
$End 
  
$Resource_type Детали : temporary
$Parameters
	номер             : integer 
    позиция_в_очереди : integer 
	состояние         : ( Пришла, В_обработке, В_наладке, Установка_базирование, Установлена, Налажена, Простой)
	номер_операции    : integer = 1
	тип               : integer 
$End

$Resource_type Приспособления : permanent
$Parameters
	состояние        		   : ( Наладка, Установка_базирование, Обработка, Свободно, Установлено, Налажено, Простой )
	тип                  	   : such_as Станки.тип
	номер_установленной_детали : such_as Детали.номер = 0
$End

