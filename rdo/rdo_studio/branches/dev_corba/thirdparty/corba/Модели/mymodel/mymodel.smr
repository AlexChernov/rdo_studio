// Имя модели (*.pat, *.rtp, *.fun и *.dpt)
Model_name     = mymodel

// Ресурсы (*.rss)
Resource_file  = mymodel

// Операции (*.opr)
OprIev_file    = mymodel

// Собираемые показатели (*.pmd)
Statistic_file = mymodel

// Результаты прогона (*.pmv), выводятся внизу экрана
Results_file   = mymodel

// Трассировка (*.trc)
// Если файл трассировки закомментировать, то отключится вся трассировка в модели
Trace_file     = mymodel

// Режим прогона, выключить анимацию
Show_mode      = NoShow
// Скорость анимации. Чем больше значение, тем быстрее прогон
// Используется только в режиме анимации
Show_rate      = 3600.0

// Условие остановки модели: одна 12-ти часовая 7-ми дневная рабочая неделя.
Terminate_if Time_now >= 12 * 7 * 60

external_model Server = RDO2