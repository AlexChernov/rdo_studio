$Resource_type Детали: permanent
$Parameters
	состояние_детали 		: ( Обрабатывается, Не_обрабатывается )
	количество_в_очереди  	: integer
	количество_обработанных	: integer = 10
	вещественный_параметр  	: real
	целый_с_диапазоном		: integer [0..1000] = 0
	вещественный_диапазон	: real [0.0..20.0] = 5
	перечислимый_умолчание	: (Свободен, Занят, Погрузка, Разгрузка) = Свободен
	//вещественный_значение  	: such_as Детали.вещественный_диапазон = 20
	перечислимый_дополнит	: such_as Детали.перечислимый_умолчание = Занят
$End

$Resource_type Дополнит		: permanent
$Parameters
	состояние_детали 		: ( Обрабатывается, Не_обрабатывается )
	количество_в_очереди  	: integer
	целый_с_диапазоном		: integer [0..1000] = 0
$End

