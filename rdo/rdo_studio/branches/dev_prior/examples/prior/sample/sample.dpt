$Decision_point приход : some
$Condition select(детали: nocheck).size() < количество_деталей
$Activities
	приход_детали: образец_прихода_детали
$End

$Decision_point обработка_деталей_типа_1 : some
$Condition NoCheck
$Priority (0.5)
$Activities
	обработка_детали_1: образец_операции_1 1
	обработка_детали_2: образец_операции_2 1
	обработка_детали_3: образец_операции_3 1
	пропуск_операции_1: образец_пропуска_операции_1 1
	пропуск_операции_2: образец_пропуска_операции_2 1
	пропуск_операции_3: образец_пропуска_операции_3 1
$End

$Decision_point обработка_деталей_типа_2 : some
$Condition NoCheck
$Priority (0.75)
$Activities
	обработка_детали_4: образец_операции_1 2
	обработка_детали_5: образец_операции_2 2
	обработка_детали_6: образец_операции_3 2
	пропуск_операции_4: образец_пропуска_операции_1 2
	пропуск_операции_5: образец_пропуска_операции_2 2
	пропуск_операции_6: образец_пропуска_операции_3 2
$End

$Decision_point обработка_деталей_типа_3 : some
$Condition NoCheck
$Priority (1.0)
$Activities
	обработка_детали_7: образец_операции_1 3
	обработка_детали_8: образец_операции_2 3
	обработка_детали_9: образец_операции_3 3
	пропуск_операции_7: образец_пропуска_операции_1 3
	пропуск_операции_8: образец_пропуска_операции_2 3
	пропуск_операции_9: образец_пропуска_операции_3 3
$End
