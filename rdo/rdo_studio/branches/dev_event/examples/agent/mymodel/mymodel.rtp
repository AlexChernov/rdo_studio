//служебный ресурс
$Resource_type системы: permanent
$Parameters
	количенство_входящих_заявок   : integer = 0
	сокрость_изготовления_валов   : integer = 50// шт/день
	сокрость_изготовления_корпусов: integer = 5 // шт/день
	сокрость_изготовления_шестер  : integer = 10// шт/день
$End

//врядли эти ресурсы понадобятся, пока что в модели их работу выполняют точки принятия решения. и это поидее нормально.
$Resource_type агенты : permanent
$Parameters
	имя_агента: (не_определен, координатор, производственный, другой)
$End

//заявки, приходящие в систему
$Resource_type входящие_заявки: temporary
$Parameters
	номер               : integer
	предмет_заказа      : (вал, корпус, шестерня, документация)
	объем_заказа        : integer
	срок_выполнения     : integer
	состояние           : (пришла, обработана) = пришла
	местоположение      : such_as агенты.имя_агента = координатор
	исполнительный_отдел: such_as агенты.имя_агента = не_определен
	резюме              : (не_определено, наша_фирма_не_предоставляет_такие_услуги, ваш_заказ_уже_готов, ваш_заказ_будет_готов_в_указанный_срок, мы_не_успеем_изготовить_ваш_заказ_в_указанный_срок) = не_определено
$End

//от наличия продукции на складе зависит исполнение заявки
$Resource_type склад_производственного_отдела: temporary
$Parameters
	вал     : integer //запас валов
	корпус  : integer //запас корпусов
	шестерня: integer //запас шестерней
$End

