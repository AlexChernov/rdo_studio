
$Process
//Появление заготовок в системе каждые 150 секунд по экспоненциальному закону
	GENERATE Интервал_прихода_заготовок(100) 
//	ASSIGNE  ROBOT.Состояние = Сломан
//	ASSIGNE  ST11.Состояние = Сломан
//Перемещение заготовки из зоны приемки заготовок к первому станку
	QUEUE	 QIN
	SEIZES	 ROBOT
	ASSIGNE  ROBOT.Положение = state1
	DEPART	 QIN
	ADVANCE  Время_захвата(8,1)
	ADVANCE  6
	ADVANCE  Время_захвата(8,1)
	RELEASES ROBOT
//	ASSIGNE  ROBOT.Положение = state4
//	RELEASE  ROBOT 
//	ASSIGNE  ROBOT.Состояние_для_анимации = деталь_на_станке_1

//Обработка на первом станке
	QUEUE	 QSTANOK1
	SEIZES	 ST11, ST12, ST13 
	DEPART	 QSTANOK1
	ADVANCE  Обработка_на_станке_1(60,10)
	RELEASES ST11, ST12, ST13 
	
//Перемещение заготовки от перового станка ко второму

	QUEUE	 QEXITST1
	SEIZES	 ROBOT
	ASSIGNE  ROBOT.Положение = state2
	DEPART	 QEXITST1
	ADVANCE  Время_захвата(8,1)
	ADVANCE  7
	ADVANCE  Время_захвата(8,1)
	RELEASES ROBOT 
//	ASSIGNE  ROBOT.Положение = state4
	
//Обработка на втором станке
	QUEUE	 QSTANOK2
	SEIZES	 STANOK2
	DEPART	 QSTANOK2
	ADVANCE  Обработка_на_станке_2(100)
	RELEASES STANOK2 

//Перемещение заготовки от второго станка в зону готовых деталей
	QUEUE	 QEXITST2
	SEIZES	 ROBOT
	ASSIGNE  ROBOT.Положение = state3
	DEPART	 QEXITST2
	ADVANCE  Время_захвата(8,1)
	ADVANCE  5
	ADVANCE  Время_захвата(8,1)
	RELEASES  ROBOT 
//	ASSIGNE  ROBOT.Положение = state4
	
	QUEUE	 QOUT
	
	TERMINATE 1 
$End
