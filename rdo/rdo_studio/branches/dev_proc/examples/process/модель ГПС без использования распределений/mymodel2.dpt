$Process
//Появление заготовок в системе каждые 150 секунд по экспоненциальному закону
	GENERATE 150

//Перемещение заготовки из зоны приемки заготовок к первому станку
	QUEUE	 QROBOT
	SEIZES	 ROBOT
	DEPART	 QROBOT
	ADVANCE  8
	ADVANCE  6
	ADVANCE  8
	RELEASES ROBOT 
	
//Обработка на первом станке
	QUEUE	 QSTANOK1
	SEIZES	 STANOK1
	DEPART	 QSTANOK1
	ADVANCE  60
	RELEASES STANOK1 
	
//Перемещение заготовки от перового станка ко второму
	QUEUE	 QSTANOK1_OUT
	SEIZES	 ROBOT
	DEPART	 QSTANOK1_OUT
	ADVANCE  8
	ADVANCE  7
	ADVANCE  8
	RELEASES ROBOT 
	
//Обработка на втором станке
	QUEUE	 QSTANOK2
	SEIZES	 STANOK2
	DEPART	 QSTANOK2
	ADVANCE  100
	RELEASES STANOK2 

//Перемещение заготовки от второго станка в зону готовых деталей
	QUEUE	 QSTANOK2_OUT
	SEIZES	 ROBOT
	DEPART	 QSTANOK2_OUT
	ADVANCE  8
	ADVANCE  5
	ADVANCE  8
	RELEASES ROBOT
	
	TERMINATE 1
$End
