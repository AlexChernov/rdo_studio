/*$Process
//Появление заготовок в системе 
	GENERATE Интервал_прихода_заготовок(150) 
//Перемещение заготовки из зоны приемки заготовок к первому станку
	QUEUE	 ВхНак
	SEIZES	 Робот
	DEPART	 ВхНак
	ASSIGNE  Робот.Положение = Положение1
	ADVANCE  Время_захвата(8,1)
	ADVANCE  6
	ADVANCE  Время_захвата(8,1)
	RELEASES Робот
//Обработка на первом станке
	QUEUE	 ВхНакСт1
	SEIZES	 Ст11, Ст12, Ст13 
	DEPART	 ВхНакСт1
	ADVANCE  Обработка_на_станке_1(60,10)
	RELEASES Ст11, Ст12, Ст13 
//Перемещение заготовки от перового станка ко второму
	QUEUE	 ВыхНакСт1
	SEIZES	 Робот
	DEPART	 ВыхНакСт1
	ASSIGNE  Робот.Положение = Положение2
	ADVANCE  Время_захвата(8,1)
	ADVANCE  7
	ADVANCE  Время_захвата(8,1)
	RELEASES Робот
//Обработка на втором станке
	QUEUE	 ВхНакСт2
	SEIZES	 Ст2
	DEPART	 ВхНакСт2
	ADVANCE  Обработка_на_станке_2(100)
	RELEASES Ст2 
//Перемещение заготовки от второго станка в зону готовых деталей
	QUEUE	 ВыхНакСт2
	SEIZES	 Робот
	DEPART	 ВыхНакСт2
	ASSIGNE  Робот.Положение = Положение3
	ADVANCE  Время_захвата(8,1)
	ADVANCE  5
	ADVANCE  Время_захвата(8,1)
	RELEASES Робот 
//Подсчет количества деталей в выходном накопителе
	QUEUE	 ВыхНак
//Удаление транзактов	
	TERMINATE 1 
$End
*/
$Process
//Появление заготовок в системе каждые 150 секунд по экспоненциальному закону
	GENERATE Интервал_прихода_заготовок(20) 
//Перемещение заготовки из зоны приемки заготовок к первому станку
	QUEUE	 ВхНак
	SEIZES	 Робот
	DEPART	 ВхНак
	ASSIGNE	 Робот.Положение = Положение1
	ADVANCE  Время_захвата(2,1)
	ADVANCE  2
	ADVANCE  Время_захвата(2,1)
	RELEASES Робот
//Обработка на первом станке
	QUEUE	 ВхНакСт1
	SEIZES	 Ст11
	DEPART	 ВхНакСт1
	ADVANCE  Обработка_на_станке_1(30,7)
	RELEASES Ст11
//Перемещение заготовки от перового станка ко второму
	QUEUE	 ВыхНакСт1
	SEIZES	 Робот
	DEPART	 ВыхНакСт1
	ASSIGNE	 Робот.Положение = Положение2
	ADVANCE  Время_захвата(2,1)
	ADVANCE  3
	ADVANCE  Время_захвата(2,1)
	RELEASES Робот 
//Обработка на втором станке
	QUEUE	 ВхНакСт2
	SEIZES	 Ст2
	DEPART	 ВхНакСт2
	ADVANCE  Обработка_на_станке_2(60)
	RELEASES Ст2
//Перемещение заготовки от второго станка в зону готовых деталей
	QUEUE	 ВыхНакСт2
	SEIZES	 Робот
	DEPART	 ВыхНакСт2
	ASSIGNE	 Робот.Положение = Положение3
	ADVANCE  Время_захвата(2,1)
	ADVANCE  4
	ADVANCE  Время_захвата(2,1)
	RELEASES Робот
//Подсчет количества деталей в выходном накопителе
	QUEUE	 ВыхНак
//Удаление транзактов	
	TERMINATE 1
$End
